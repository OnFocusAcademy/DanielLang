(module Base
    (define (inc x) (+ x 1))
    (define (dec x) (- x 1))
    (define (not x)
        (if x false true))
    (defmacro (unless pred a b) `(if ~pred ~b ~a))
    (defmacro (and a b) `(if ~a ~b false))
    (defmacro (or a b) `(if ~a ~a ~b))
    (provide inc)
    (provide dec)
    (provide not)
    (provide unless)
    (provide and)
    (provide or))
